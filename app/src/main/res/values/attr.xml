<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ValueText">
        <attr name="android:drawableLeft" />
        <attr name="android:drawableRight" />
        <attr name="android:drawablePadding" />
        <attr name="android:singleLine" />
        <attr name="android:text" />
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="textMargin" format="dimension" />
        <attr name="lineSpace" format="dimension" />
        <attr name="title" format="string" />
        <attr name="titleColor" format="color" />
        <attr name="titleSize" format="dimension" />
        <attr name="textAlign" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>


    <declare-styleable name="RoundBtn">
        <attr name="android:checked" />
        <attr name="android:text" />
        <attr name="android:textSize" />

        <attr name="sub1" format="string" />
        <attr name="sub2" format="string" />
        <attr name="subSize" format="dimension" />
        <attr name="sub2Size" format="dimension" />
        <attr name="subColor" format="color" />

        <attr name="bgColor" format="color" />
        <attr name="startColor" format="color" />
        <attr name="centerColor" format="color" />
        <attr name="endColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RadioBtn">
        <attr name="android:checked" />

        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="android:textSize" />

        <attr name="android:startColor" />
        <attr name="android:centerColor" />
        <attr name="android:endColor" />

        <attr name="desc" format="string" />
        <attr name="descColor" format="color" />
        <attr name="descSize" format="dimension" />

    </declare-styleable>

    <declare-styleable name="TabView">
        <attr name="android:text" />
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:checked" />
        <attr name="android:drawablePadding" />
        <attr name="checkImg" format="reference" />
        <attr name="unCheckImg" format="reference" />
    </declare-styleable>

    <declare-styleable name="CheckColorSelect">
        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="android:checked" />
    </declare-styleable>

    <declare-styleable name="CheckDrawableSelect">
        <attr name="android:checked" />
        <attr name="android:src" />
    </declare-styleable>


    <declare-styleable name="ProgressChart">
        <attr name="android:text" />
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="title" />
        <attr name="titleColor" />
        <attr name="titleSize" />
        <attr name="stroke" format="dimension" />
        <attr name="progressColor" format="color" />
        <attr name="progress" format="float" />
    </declare-styleable>

    <declare-styleable name="AnimProgressBar">
        <attr name="progressBackColor" format="color" />
        <attr name="gradientColor" format="color" />
        <attr name="progressColor" />
        <attr name="progress" />
    </declare-styleable>

    <declare-styleable name="GradeChart">
        <attr name="colors" format="string" />
        <attr name="gap" format="dimension" />
        <attr name="radius" format="dimension" />
    </declare-styleable>


</resources>